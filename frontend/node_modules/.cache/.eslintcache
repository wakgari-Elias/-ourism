src/shared/NewsLetter.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/index.js",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/App.js",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/components/Layout/Layout.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/router/Routers.js",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/components/Footer/Footer.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/components/Header/Header.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/Tours.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/TourDetails.jsx",["258","259"],[],"import React,{useRef,useState} from 'react'\nimport '../styles/tour-details.css'\nimport {Container, Row,Col,Form,ListGroup} from 'reactstrap'\n\nimport {useParams} from 'react-router-dom'\nimport tourData from '../assets/data/tours'\nimport calculateAvgRating from '../utils/avgRating'\nimport avatar from '../assets/images/avatar.jpg';\nimport Booking from '../components/Booking/Booking'\n\nconst TourDetails = () => {\n\n  const {id} = useParams();\n const reviewMsRef=useRef('')\n\n\n const [tourRating, setTourRating] = useState(null);\n const handleRatingClick = (rating) => {\n  setTourRating(rating);\n  // Update UI or perform other actions based on the selected rating\n};\n\n\n\n  // This is an Static Data  later we will  call our API and load our data from database\n  const tour =tourData.find(tour=> tour.id===id)\n\n  // destructure props from tour object\n  const {photo,title ,desc, price, address, reviews,city,distance, maxGroupSize} = tour;\n\n  const {totalRating,avgRating} = calculateAvgRating (reviews);\n\n\n  // format Rating \n  const options = { day:\"numeric\", month:\"long\" , year:\"numeric\" };\n\n  // submit request to the server \n  const submitHandler = e =>{\n    e.preventDefault()\n    const reviewText =reviewMsRef.current.value\n    // alert(`${reviewText}, ${tourRating}`);\n\n    // later we will call our API to save the review\n\n  }\n\n\n\n\n\n\n  return <>\n   <section>\n    <Container>\n      <Row>\n        <Col lg='8'>'\n        <div  className='tour__content'>\n          <img src={photo} alt=\"Pictures\"/>\n          <div className='tour__info'>\n            <h2>{title}</h2>\n\n              <div className='d-flex align-items-center gap-5'>\n              <span className='tour__location d-flex align-items-center gap-1'>\n            <i class=\"ri-star-fill\" style={{color:\"var(--secondary-color)\"}}>\n            </i>{avgRating === 0 ? null : avgRating}\n          {totalRating === 0 ? 'Not Rated' : <span> ({reviews?.length})</span> }\n          </span>\n          <span>\n          <i class=\"ri-map-pin-user-line\"></i> {address}\n          </span>\n           </div>\n\n   <div className='tour__extra-details'>\n    <span><i class=\"ri-map-pin-2-line\"></i> {city} </span>\n\n    <span>   <i class=\"ri-money-dollar-circle-line\"></i> ${price}/per person </span>\n    <span>   <i class=\"ri-map-pin-time-line\"></i>{distance}K/m </span>\n\n\n    <span>  <i class=\"ri-group-line\"></i> {maxGroupSize} people </span>\n   </div>\n\n   <h5>Description</h5>\n   <p> {desc}</p>\n   </div>\n  {/* ============= Tour Reviews Section ============ */}\n <div className='tour__reviews mt-4'>\n  <h4>Reviews ({reviews?.length} reviews)</h4>\n\n  <Form onSubmit={submitHandler}>\n<div  className='d-flex align-items-center gap-3 mb-4 rating__group'>\n  <span onClick={()=>handleRatingClick(1)}> 1<i class=\"ri-star-s-fill\"></i> </span>\n  <span  onClick={()=>handleRatingClick(2)}> 2<i class=\"ri-star-s-fill\"></i> </span>\n  <span onClick={()=>handleRatingClick(3)} > 3<i class=\"ri-star-s-fill\"></i> </span>\n  <span onClick={()=>handleRatingClick(4)}> 4<i class=\"ri-star-s-fill\"></i> </span>\n  <span onClick={()=>handleRatingClick(5)}> 5<i class=\"ri-star-s-fill\"></i>  </span>\n</div>\n\n <div className='review__input'>\n  <input \n  type='text'  \n  ref={reviewMsRef} \n  placeholder='Share us you Thoughts' \n  required\n   />\n  <button className='btn primary__btn  text-white' type='submit'>\n    Submit\n  </button>\n\n </div>\n  </Form>\n\n\n<ListGroup className='user__reviews'>\n  {\n    reviews?.map(review=>(\n      <div className='review__item'>\n        <img src={avatar} alt=\"\" />\n\n        <div className='w-100'>\n          <div className='d-flex align-items-center justify-content-between'>\n\n          <div>\n            <h5>Elias</h5>\n            <p>\n              {\n                new Date (\"12-07-2024\").toLocaleDateString(\n                  \"en-US\",\n                  options\n                )}\n            </p>\n          </div>\n          <span className='d-flex align-items-center'>\n            5<i class=\"ri-star-s-fill\"></i>\n          </span>\n          </div>\n\n          <h6>Amazing Tour</h6>\n        </div>\n      </div>\n    ))\n  }\n\n</ListGroup>\n\n </div>\n\n  {/* ============= Tour Reviews Section End ============ */}\n        </div>\n        </Col>\n\n        <Col lg='4'>\n      <Booking tour={tour}  avgRating={avgRating}/>\n        \n        </Col>\n      </Row>\n    </Container>\n   </section>\n\n  </>\n    \n  \n}\n\nexport default TourDetails\n",[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/Home.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/Login.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/SearchResultList.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/Pages/Register.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/services/ServiceList.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/shared/SearchBar.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/shared/Subtitle.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/shared/NewsLetter.jsx",[],[],"/home/elias/Desktop/Ethiourism/frontend/src/components/Featured-tours/FeaturedTourList.jsx",[],[],[],"/home/elias/Desktop/Ethiourism/frontend/src/components/image-gallery/MasonryImagesGallery.jsx",[],[],"/home/eli